# Firebase Functions Environment Variables
# Copy this file to .env and fill in your actual values
# IMPORTANT: Replace all placeholder values with your actual credentials for local development

# Google Play Configuration
# Replace with your actual app package name
GOOGLE_PLAY_PACKAGE_NAME=YOUR_APP_PACKAGE_NAME

# Replace with your actual Google Play service account JSON key
# Download from Google Play Console > Setup > API access > Service accounts
GOOGLE_PLAY_SERVICE_ACCOUNT_KEY=YOUR_SERVICE_ACCOUNT_JSON

# API Configuration
# Replace with your actual Rork API URL if different
RORK_API_URL=YOUR_RORK_API_URL

# Development flags
NODE_ENV=development

# Note: In production, use Firebase Functions config instead:
# firebase functions:config:set googleplay.package_name="your-actual-package-name"
# firebase functions:config:set googleplay.service_account_key='{"type":"service_account","project_id":"your-project-id",...}'
# firebase functions:config:set rork.api_url="your-actual-api-url"
