# Firebase Functions Environment Variables
# Copy this file to .env and fill in your actual values
# IMPORTANT: Replace all placeholder values with your actual credentials

# Google Play Configuration
GOOGLE_PLAY_PACKAGE_NAME=YOUR_APP_PACKAGE_NAME
GOOGLE_PLAY_SERVICE_ACCOUNT_KEY=YOUR_SERVICE_ACCOUNT_JSON

# API Configuration
RORK_API_URL=YOUR_RORK_API_URL

# Development flags
NODE_ENV=development

# Note: In production, use Firebase Functions config instead:
# firebase functions:config:set googleplay.package_name="com.auravisionai.auravisionai"
# firebase functions:config:set googleplay.service_account_key='{"type":"service_account",...}'